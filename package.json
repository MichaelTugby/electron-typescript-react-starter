{
  "name": "electron-typescript-vue-starter",
  "description": "A custom built electron typescript and vue webpack build",
  "main": "dist/main.js",
  "version": "0.1.0",
  "scripts": {
    "dev": "ts-node webpack.devServer.ts",
    "build": "run-s build:all && electron-packager ./ --out=releases --overwrite",
    "build:all": "rm -rf ./dist && cross-env NODE_ENV=production run-p build:main build:preload build:renderer",
    "build:main": "webpack --config webpack.main.config.ts",
    "build:preload": "webpack --config webpack.preload.config.ts",
    "build:renderer": "webpack --config webpack.renderer.config.ts",
    "test": "jest src/**/*.test.{ts,tsx} --coverage"
  },
  "devDependencies": {
    "@babel/core": "^7.13.8",
    "@babel/preset-env": "^7.13.9",
    "@babel/preset-typescript": "^7.13.0",
    "@jest/types": "^26.6.2",
    "@types/csp-html-webpack-plugin": "^3.0.0",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/jest": "^26.0.20",
    "@types/mini-css-extract-plugin": "^1.2.2",
    "@types/terser-webpack-plugin": "^5.0.2",
    "@types/webpack-bundle-analyzer": "^3.9.1",
    "@types/webpack-dev-server": "^3.11.1",
    "@typescript-eslint/eslint-plugin": "^4.16.1",
    "@typescript-eslint/parser": "^4.16.1",
    "@vue/compiler-sfc": "^3.0.7",
    "babel-loader": "^8.2.2",
    "core-js": "^3.9.1",
    "cross-env": "^7.0.3",
    "csp-html-webpack-plugin": "^5.1.0",
    "css-loader": "^5.1.1",
    "debug": "^4.3.1",
    "electron": "^12.0.0",
    "electron-connect": "^0.6.3",
    "electron-devtools-installer": "^3.1.1",
    "electron-packager": "^15.2.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.7.0",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "html-webpack-plugin": "^5.2.0",
    "jest": "^26.6.3",
    "mini-css-extract-plugin": "^1.3.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "terser-webpack-plugin": "^5.1.1",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "4.1.5",
    "vue-eslint-parser": "^7.6.0",
    "vue-loader": "next",
    "vue-style-loader": "^4.1.3",
    "vue-template-compiler": "^2.6.12",
    "webpack": "^5.24.2",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "vue": "next",
    "vuex": "next"
  }
}
