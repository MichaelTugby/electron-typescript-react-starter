{
  "name": "id3-tag-editor",
  "main": "dist/main.bundle.js",
  "version": "0.1.0",
  "scripts": {
    "start": "ts-node webpack.devServer.ts",
    "build": "cross-env NODE_ENV=production run-p build:main build:renderer && electron-builder",
    "build:main": "webpack --config webpack.main.config.ts",
    "build:renderer": "webpack --config webpack.renderer.config.ts",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "id3-tag-editor",
    "mac": {
      "category": "music"
    },
    "files": [
      "./dist/**/*"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.1.4",
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@emotion/babel-plugin": "^11.1.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.4.3",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@types/webpack-dev-server": "^3.11.1",
    "babel-loader": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^11.2.1",
    "electron-builder": "^22.9.1",
    "electron-connect": "^0.6.3",
    "electron-devtools-installer": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^6.1.0",
    "html-webpack-plugin": "^4.5.1",
    "npm-run-all": "^4.1.5",
    "react-refresh": "^0.9.0",
    "ts-node": "^9.1.1",
    "tsconfig-paths": "^3.9.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^5.17.0",
    "webpack-cli": "^4.4.0",
    "webpack-dev-server": "^3.11.2"
  }
}
