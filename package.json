{
  "name": "id3-tag-editor",
  "main": "dist/main.js",
  "version": "0.1.0",
  "scripts": {
    "start": "ts-node webpack.devServer.ts",
    "build": "run-s build:all && electron-packager ./ --out=releases --overwrite",
    "build:all": "rm -rf ./dist && cross-env NODE_ENV=production run-p build:main build:renderer",
    "build:main": "webpack --config webpack.main.config.ts",
    "build:renderer": "webpack --config webpack.renderer.config.ts",
    "test": "jest test/unit/*.test.tsx --coverage",
    "e2e": "run-s build:all && jest test/e2e/*.test.ts"
  },
